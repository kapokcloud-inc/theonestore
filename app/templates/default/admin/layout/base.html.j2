{% import '/default/admin/libs/libs.html.j2' as libs %}
<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="description" content="">
  <meta name="author" content="">
  <link rel="icon" type="image/png" sizes="16x16" href="/static/default/admin/plugins/images/favicon.png">
  <title>{{ g.page_title }} - {{_('一店')}}</title>
  <!-- Bootstrap Core CSS -->
  <link href="/static/default/admin/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
  <!-- Menu CSS -->
  <link href="/static/default/admin/plugins/bower_components/sidebar-nav/dist/sidebar-nav.min.css" rel="stylesheet">
  <!-- toast CSS -->
  <link href="/static/default/admin/plugins/bower_components/toast-master/css/jquery.toast.css" rel="stylesheet">
  <!-- morris CSS -->
  <link href="/static/default/admin/plugins/bower_components/morrisjs/morris.css" rel="stylesheet">
  <!-- Footable CSS -->
  <link href="/static/default/admin/plugins/bower_components/footable/css/footable.core.css" rel="stylesheet">
  <!-- animation CSS -->
  <link href="/static/default/admin/css/animate.css" rel="stylesheet">
  <!-- Custom CSS -->
  <link href="/static/default/admin/css/style.css" rel="stylesheet">
  <!-- color CSS -->
  <link href="/static/default/admin/css/colors/megna.css" id="theme" rel="stylesheet">

  {% block head %}{% endblock %}
</head>

<body class="fix-header">
  <!-- Preloader -->
  <div class="preloader">
    <svg class="circular" viewBox="25 25 50 50">
      <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" />
    </svg>
  </div>
  <!-- Wrapper -->
  <div id="wrapper">
    {% include '/default/admin/layout/top.html.j2' %}
    {% include '/default/admin/layout/nav.html.j2' %}
    {% block body %}{% endblock %}
    {% include '/default/admin/layout/footer.html.j2' %}
  </div>
  <!-- End Wrapper -->
  <!-- All Jquery -->
  <script src="/static/default/admin/plugins/bower_components/jquery/dist/jquery.min.js"></script>
  <!-- Bootstrap Core JavaScript -->
  <script src="/static/default/admin/bootstrap/dist/js/bootstrap.min.js"></script>
  <!-- Menu Plugin JavaScript -->
  <script src="/static/default/admin/plugins/bower_components/sidebar-nav/dist/sidebar-nav.min.js"></script>
  <!--slimscroll JavaScript -->
  <script src="/static/default/admin/js/jquery.slimscroll.js"></script>
  <!--Wave Effects -->
  <script src="/static/default/admin/js/waves.js"></script>
  <!--Counter js -->
  <script src="/static/default/admin/plugins/bower_components/waypoints/lib/jquery.waypoints.js"></script>
  <script src="/static/default/admin/plugins/bower_components/counterup/jquery.counterup.min.js"></script>
  <!-- Custom Theme JavaScript -->
  <script src="/static/default/admin/js/custom.min.js"></script>
  <script src="/static/default/admin/plugins/bower_components/toast-master/js/jquery.toast.js"></script>
  <!--Style Switcher -->
  <script src="/static/default/admin/plugins/bower_components/styleswitcher/jQuery.style.switcher.js"></script>

  {% block footer %}{% endblock %}
