{% extends '/default/pc/layout/base.html.j2' %}


{% block head %}
<title>{{_('我的优惠券 - 一店')}}</title>
<link rel="stylesheet" href="/static/default/pc/css/order.css">
<link rel="stylesheet" href="/static/default/pc/css/cart.css">
<link rel="stylesheet" href="/static/default/pc/iconfont/iconfont.css">
{% endblock %}

{% block body %}
<div class="index_wrap clearfix">
  <div class="type-head">
    <div class="head-inner">
      <p class="type-head_text">
        <span>{{_('我的优惠券')}}</span>
      </p>
    </div>
    <div class="user-main">
      <div class="head-inner clearfix">
        {%include 'default/pc/layout/nav_left.html.j2'%}
        <div class="order-wrap">
          <div class="allPad">
            <div class="box-hd">
              <ul class="box-hd_list">
                <li class="box-hd_item">
                  <a class="allOn" href="">{{_('未使用')}}</a>
                </li>
                <li class="box-hd_item">
                  <a class="" href="">{{_('已使用')}}</a>
                </li>
                <li class="box-hd_item">
                  <a class="" href="">{{_('已失效')}}</a>
                </li>
              </ul>
            </div>
            <!-- 有优惠券 -->
            <div class="order-bd pad-top40">
              <ul class="coupon-box_list clearfix">
                <li class="coupon-box_item">
                  <div class="coupon-info">
                    <div class="wave"></div>
                    <p>
                      <span class="coupon-amount">￥20</span>
                      满100减20
                    </p>
                    <p>{{_('有效期')}}：2018.07.12</p>
                  </div>
                  <div class="all-store">{{_('[全场通用]')}}</div>
                </li>
                <li class="coupon-box_item border-eee">
                  <div class="coupon-info bg-eee">
                    <div class="wave"></div>
                    <p>
                      <span class="coupon-amount">￥20</span>
                      满100减20
                    </p>
                    <p>{{_('有效期')}}：2018.07.12</p>
                  </div>
                  <div class="all-store color-eee">{{_('[全场通用]')}}</div>
                </li>
                <li class="coupon-box_item">
                  <div class="coupon-info">
                    <div class="wave"></div>
                    <p>
                      <span class="coupon-amount">￥20</span>
                      满100减20
                    </p>
                    <p>{{_('有效期')}}：2018.07.12</p>
                  </div>
                  <div class="all-store">{{_('[全场通用]')}}</div>
                </li>
                <li class="coupon-box_item border-eee">
                  <div class="coupon-info bg-eee">
                    <div class="wave"></div>
                    <p>
                      <span class="coupon-amount">￥20</span>
                      满100减20
                    </p>
                    <p>{{_('有效期')}}：2018.07.12</p>
                  </div>
                  <div class="all-store color-eee">{{_('[全场通用]')}}</div>
                </li>
              </ul>
              <div class="page-box" style="margin-top:30px;">
                <span class="page-num page-first">
                  <i class="icon-xiaoyu"></i>
                </span>
                <span class="page-num btn-current">1</span>
                <a href="" class="page-num">2</a>
                <a href="" class="page-num">3</a>
                <a href="" class="page-num">4</a>
                <a href="" class="page-num">5</a>
                <a href="" class="page-num">6</a>
                <span class="page-more">...</span>
                <a href="" class="page-num">12</a>
                <a href="" class="page-num page-last">
                  <i class="icon-dayu"></i>
                </a>
              </div>
            </div>
            <!-- 有优惠券 end -->

            <!-- 没有优惠券 -->
            <!-- <div class="not-one">
              <p class="not-one_text">{{_('未找到相应优惠券!')}}</p>
              <a class="not-one_btn btn-primary" href="{{ url_for('pc.index.root') }}">{{_('到商场去看看')}}</a>
            </div> -->
            <!-- 没有优惠券 end -->
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
{% endblock %}

{% block script %}
<script type="text/javascript"></script>
{% endblock%}